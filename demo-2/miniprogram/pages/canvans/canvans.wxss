/* pages/index/vfblock.wxss */

.container {
  position: relative;
  overflow: hidden;
  margin-top: 100px;
}

canvas {
  top: 0
}

.canvas {
  left: -9999px;
  position: absolute;
  z-index: -1;
}

.block {
  width: 100vw;
  height: 30vh;
  position: absolute;
}

.three {
  position: absolute;
  width: 50px;
  height: 50px;
  z-index: 0;
  border: 1px dashed rgb(252, 252, 252);

}

.view {
  position: absolute;
  text-align: center;
  height: 100rpx;
  width: 100rpx;
  line-height: 100rpx;
  background: #1AAD19;
  color: #fff;
  border-radius: 10rpx;
}

area {
  height: 100rpx;
  width: 100%;
  margin: 50rpx;
  background-color: rgb(240, 236, 236);
  overflow: hidden;
  border-radius: 10rpx;
  border: 1px solid rgb(139, 139, 139);
}