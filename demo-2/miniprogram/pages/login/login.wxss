page {
  background-color: #f2f2f2;
}

.login {
  height: 100%;
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: center;
  /* box-sizing: border-box; */
  padding: 5px 10px;
  align-items: center;
}

.shuoming {
  display: block;
  margin: 10px 0;
  color: gray;
  font-size: 14px;
}

.mysubmit {
  width: 100%;
}

.form-wrap {
  border-top: 1px solid, #eee;
  margin: 10px 0;
}

.flex-wrap {
  display: flex;
  flex-direction: row;
}

.input-wrap {
  background-color: #fff;
  border-bottom: 1px solid #eee;
  margin: 0 -12px;
  padding: 0 12px;
  height: 46px;
  line-height: 46px;
}

.label {
  color: #999;
  width: 4.5em;
}

.account-info-input {
  font-size: 18px;
  line-height: 100%;
  height: 100%;
  width: 100%;
}
/* pages/index/vfblock.wxss */

.container {
  position: relative;
  overflow: hidden;
  margin-top: 100px;
}

canvas {
  top: 0
}

.canvas {
  left: -9999px;
  position: absolute;
  z-index: -1;
}

.block {
  width: 100vw;
  height: 30vh;
  position: absolute;
}

.three {
  position: absolute;
  width: 50px;
  height: 50px;
  z-index: 0;
  border: 1px dashed rgb(252, 252, 252);
}

.view {
  position: absolute;
  text-align: center;
  height: 100rpx;
  width: 100rpx;
  line-height: 100rpx;
  background: #1AAD19;
  color: #fff;
  border-radius: 10rpx;
}

area {
  height: 100rpx;
  width: 100%;
  margin: 50rpx;
  background-color: rgb(240, 236, 236);
  overflow: hidden;
  border-radius: 10rpx;
  border: 1px solid rgb(139, 139, 139);
}
.contenter{
  display: flex;
  flex-direction: column;
}